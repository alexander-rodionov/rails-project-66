{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@rails/ujs": "^7.1.3-4",
    "autoprefixer": "^10.4.21",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "sass": "^1.89.0"
  },
  "scripts": {
    "build:css:compile": "sass -q ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --load-path=node_modules",
    "build:css:prefix": "postcss ./app/assets/builds/application.css --use=autoprefixer --output=./app/assets/builds/application.css",
    "build:css": "yarn build:css:compile && yarn build:css:prefix",
    "watch:css": "nodemon --watch ./app/assets/stylesheets/ --ext scss --exec \"yarn build:css\"",
    "build": "yarn build:css && rake post_generate:add_frozen_string_literal && cat /project/Gemfile.lock"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "eslint": "^9.27.0",
    "globals": "^16.1.0"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}